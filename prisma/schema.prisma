generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./database.db"
}

model User {
  id       String  @id @default(uuid())
  name     String
  email    String  @unique
  password String
  role     Role    @default(USER)
}

model Professional {
  id             String @id @default(uuid())
  name           String
  email          String @unique
  qualifications String
}

enum Role {
  USER
  ADMIN
}

